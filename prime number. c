#include<stdio.h>
int main()
{
    int i, j, count=10, factor;

    for(i = 1; i <= count; i++)
    {
        factor=0;
                         for (j=1; j <= count; j++)
                        {
                            if (i%j==0)
                            {
                               factor++;
                            }
                         }
            if (factor==2)
            {
              printf("%d\n",i);
            }
    }
return 0;
}


